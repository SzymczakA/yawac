
config wifi-device 'radio0'
	option type 'mac80211'
	option path '10180000.wmac'
	option country 'CN'
	option txpower '20'
	option hwmode '11ng'
	option htmode 'HT40+'
	option noscan '1'
	option channel '1'

config wifi-iface
	option device 'radio0'
	option network 'lan'
	option mode 'ap'
	option key 'goodlife'
	option encryption 'psk-mixed'
	option ifname 'wlan0'
	option ssid 'GL-MT300N-175'
	option wds '1'

config wifi-device 'radio1'
	option type 'mac80211'
	option channel '11'
	option hwmode '11g'
	option path '101c0000.ehci/usb1/1-1/1-1:1.0'
	option htmode 'HT20'
	option txpower '20'
	option country 'CN'

config wifi-iface
	option device 'radio1'
	option mode 'sta'
	option network 'wwan'
	option ssid 'MonitoringCP'
	option encryption 'psk2'
	option key '0bcababa'

